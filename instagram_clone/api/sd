        return true;
    }
    public boolean validar(){
        if(this.textNomeUsuario.getText().toString().length() >= 3  
        || this.textUsername.getText().toString().length() >= 3
        || this.textSenha.getText().toString().length() >= 3
        || this.textBairro.getText().toString().length() >= 2
        || this.textCep.getText().toString().length() >= 3
        || this.textCidade.getText().toString().length() >= 2
        || this.textLougradouro.getText().toString().length() >= 1
        || this.textNumero.getText().toString().length() >= 1
                ) {
            return false;
        }
        return true;
    }




